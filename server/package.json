    "archiver": "^7.0.1",
    "cors": "^2.8.5",
    "cryptopan": "^0.3.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "fs-extra": "^11.3.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "PALoSA-refined": "file:..",
    "pseudonymous-id-generator": "^1.0.3",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
